mkdir $1
cd $1

mkdir dir1
touch dir1/fileA
echo "fileA!" > dir1/fileA

mkdir dir2
touch dir2/fileB
echo "Hi" > dir2/fileB

chmod 640 dir2/fileB
chmod 755 dir2

mkdir dir1/dir3
touch dir1/dir3/fileC
echo "Bye" > dir1/dir3/fileC

mkdir dir1/dir3/dir4

chmod 500 dir1/dir3/dir4
chmod 600 dir1/dir3/fileC
chmod 000 dir1/dir3
chmod 600 dir1/fileA
chmod 700 dir1
